<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.CheckedOutBooksController">
   <children>
      <TableView fx:id="table" layoutY="60.0" onMouseClicked="#showOverview" onMousePressed="#showOverview" prefHeight="341.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="authorColumn" prefWidth="75.0" text="Author" />
            <TableColumn fx:id="hardcoverCheckedOutColumn" prefWidth="116.0" text="Harcover Copies" />
            <TableColumn fx:id="paperbackCheckedOutColumn" prefWidth="127.0" text="Paperback Copies" />
            <TableColumn fx:id="seriesColumn" minWidth="7.0" prefWidth="64.0" text="Series" />
            <TableColumn prefWidth="75.0" text="Due Date" />
        </columns>
      </TableView>
      <Button fx:id="backToBrowse" layoutX="473.0" layoutY="20.0" mnemonicParsing="false" onAction="#goBack" prefHeight="26.0" prefWidth="113.0" text="Back to Browse" />
      <Label layoutX="14.0" layoutY="22.0" text="Checked Out Books">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
