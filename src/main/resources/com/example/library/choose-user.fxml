<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="356.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.ChooseUserController">
   <children>
      <Label layoutX="63.0" layoutY="132.0" text="Username:" />
      <TextField fx:id="username" layoutX="146.0" layoutY="128.0" />
      <Button fx:id="back" layoutX="280.0" layoutY="346.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Label layoutX="63.0" layoutY="45.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="login" layoutX="202.0" layoutY="187.0" mnemonicParsing="false" onAction="#setLogin" text="Login" />
      <Button fx:id="signUp" layoutX="219.0" layoutY="44.0" mnemonicParsing="false" onAction="#setSignUp" text="Sign Up" />
      <Label fx:id="message" layoutX="69.0" layoutY="262.0" prefHeight="17.0" prefWidth="146.0" />
   </children>
</AnchorPane>
